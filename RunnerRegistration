let raceNumber = Math.floor(Math.random() * 1000);
const timely = false;
const runnerAge = 18;

if (timely === true && runnerAge > 18) {
  raceNumber += 1000;
  console.log(`You will race at 9:30 am under the number ${raceNumber}`)
} else if (runnerAge > 18 && timely === false){
  console.log(`You will race at 11:00 am under the number ${raceNumber}`)
} else if (runnerAge < 18) {
  console.log(`You will race at 12:30 pm under the number ${raceNumber}`)
} else {
  console.log('Please see the registration desk')
}

